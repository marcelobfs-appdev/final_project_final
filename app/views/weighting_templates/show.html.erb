<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Weighting #<%= @weighting.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/weightings" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <!-- Edit link weighting start -->
      <div class="col">
        <a href="/weightings/<%= @weighting.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit weighting
        </a>
      </div>
      <!-- Edit link weighting end -->

      <!-- Delete link weighting start -->
      <div class="col">
        <a href="/delete_weighting/<%= @weighting.id %>" class="btn btn-block btn-outline-secondary">
          Delete weighting
        </a>
      </div>
      <!-- Delete link weighting end -->
    </div>

    <dl>
      <dt>
        Cattle
      </dt>
      <!-- Display cattle_id start -->
      <dd>
        <%= @weighting.cattle_id %>
      </dd>
      <!-- Display cattle_id end -->

      <dt>
        Brinco
      </dt>
      <!-- Display brinco start -->
      <dd>
        <%= @weighting.brinco %>
      </dd>
      <!-- Display brinco end -->

      <dt>
        Lote
      </dt>
      <!-- Display lote start -->
      <dd>
        <%= @weighting.lote %>
      </dd>
      <!-- Display lote end -->

      <dt>
        Peso
      </dt>
      <!-- Display peso start -->
      <dd>
        <%= @weighting.peso %>
      </dd>
      <!-- Display peso end -->

      <dt>
        Created at
      </dt>
      <!-- Display created_at start -->
      <dd>
        <%= time_ago_in_words(@weighting.created_at) %> ago
      </dd>
      <!-- Display created_at end -->

      <dt>
        Updated at
      </dt>
      <!-- Display updated_at start -->
      <dd>
        <%= time_ago_in_words(@weighting.updated_at) %> ago
      </dd>
      <!-- Display updated_at end -->
    </dl>
  </div>
</div>

<!-- Show Page Customization starts -->

<!-- Show Page Customization end -->
