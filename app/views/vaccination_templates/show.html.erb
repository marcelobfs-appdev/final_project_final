<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Vaccination #<%= @vaccination.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/vaccinations" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <!-- Edit link vaccination start -->
      <div class="col">
        <a href="/vaccinations/<%= @vaccination.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit vaccination
        </a>
      </div>
      <!-- Edit link vaccination end -->

      <!-- Delete link vaccination start -->
      <div class="col">
        <a href="/delete_vaccination/<%= @vaccination.id %>" class="btn btn-block btn-outline-secondary">
          Delete vaccination
        </a>
      </div>
      <!-- Delete link vaccination end -->
    </div>

    <dl>
      <dt>
        Cattle
      </dt>
      <!-- Display cattle_id start -->
      <dd>
        <%= @vaccination.cattle_id %>
      </dd>
      <!-- Display cattle_id end -->

      <dt>
        Brinco
      </dt>
      <!-- Display brinco start -->
      <dd>
        <%= @vaccination.brinco %>
      </dd>
      <!-- Display brinco end -->

      <dt>
        Tipo vacina
      </dt>
      <!-- Display tipo_vacina start -->
      <dd>
        <%= @vaccination.tipo_vacina %>
      </dd>
      <!-- Display tipo_vacina end -->

      <dt>
        Created at
      </dt>
      <!-- Display created_at start -->
      <dd>
        <%= time_ago_in_words(@vaccination.created_at) %> ago
      </dd>
      <!-- Display created_at end -->

      <dt>
        Updated at
      </dt>
      <!-- Display updated_at start -->
      <dd>
        <%= time_ago_in_words(@vaccination.updated_at) %> ago
      </dd>
      <!-- Display updated_at end -->
    </dl>
  </div>
</div>

<!-- Show Page Customization starts -->
<!-- Show Page Customization end -->
